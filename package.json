{
  "name": "vue-socket-chat",
  "version": "1.0.0",
  "description": "A web chat application. Vue + graphql + socket.io",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently --names \"server, client\" \"npm run server --silent\" \"npm run client --silent\""
  },
  "author": "Reh4m",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.11.0",
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.2.1",
    "mongoose": "^5.9.4",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
